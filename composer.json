{
    "name": "umpirsky/extraload",
    "description": "Powerful ETL library.",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "convert",
        "load",
        "import",
        "export"
    ],
    "homepage": "http://umpirsky.com",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "symfony/event-dispatcher": "^3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "behat/behat": "v3.1.0rc1",
        "phpunit/phpunit": "^5.1",
        "symfony/console": "^3.0",
        "doctrine/dbal": "^2.5"
    },
    "suggest": {
        "doctrine/dbal": "Allows loading data into database using DBAL loader",
        "symfony/console": "Allows displaying tabular data in console"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "Extraload\\": "src/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    }
}
